<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/app/calendarParse.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>viewCalendar</from-outcome>
            <to-view-id>/app/extender.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>authenticate</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/app/inputCalendarEntry.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addEvent</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/app/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>viewCalendar</from-outcome>
            <to-view-id>/app/extender.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/app/calendarEmpty.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>createEmptyCalendar</from-outcome>
            <to-view-id>/app/extender.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
